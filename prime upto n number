def is_prime(num):
    if num <= 1:
        return False
    for i in range(2, num):
        if num % i == 0:
            return False
    return True

def primes_up_to_n(n):
    primes = []
    for i in range(2, n + 1):  # Start from 2 and go up to n
        if is_prime(i):
            primes.append(i)  # Add prime numbers to the list
    return primes

if __name__ == "__main__":
    n = 10  # Define the range
    result = primes_up_to_n(n)  # Get all primes up to n
    print("Prime numbers up to", n, "are:", result)
